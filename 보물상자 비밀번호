#include<iostream>
#include<algorithm>
#include<set>
#include<sstream>
using namespace std;

int main() {
    int t;
    cin >> t;
    for (int z = 0; z < t; z++) {
    	set<string, greater<>> s;
    	string rot;
    	int n, k;
        cin >> n >> k;
        cin >> rot;

        for (int i = 0; i < n / 4; i++) {
            for (int j = 0; j < 4; j++) {
                string tmp;
                for (int k = 0; k < n / 4; k++)
                    tmp += rot[(j * (n / 4) + k) % n];
                s.insert(tmp);
            }
            rot = rot[rot.length() - 1] + rot.substr(0, rot.length() - 1);
        }
        for (int i = 1; i < k; i++)
            s.erase(s.begin());
        rot = *s.begin();
        istringstream iss(rot);
        long long answer;
        iss >> hex >> answer;
        cout << "#" << z + 1 << " " << answer << '\n';
    }
    return 0;
}
